{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "lab\\.harrychang\\.me"
        }
      ],
      "destination": "/lab/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www\\.lab\\.harrychang\\.me"
        }
      ],
      "destination": "/lab/:path*"
    }
  ],
  "headers": [
    {
      "source": "/lab/:path*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
